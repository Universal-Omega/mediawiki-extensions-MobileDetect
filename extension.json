{
	"name": "MobileDetect",
	"version": "2.2",
	"author": [
		"Matthew Tran",
		"[https://mediawiki.org/wiki/User:Sophivorus Felipe Schenone]",
		"Universal Omega"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:MobileDetect",
	"descriptionmsg": "mobiledetect-desc",
	"license-name": "GPL-3.0-only",
	"type": "parserhook",
	"requires": {
		"MediaWiki": ">= 1.29.0"
	},
	"MessagesDirs": {
		"MobileDetect": [
			"i18n"
		]
	},
	"AutoloadClasses": {
		"MobileDetect": "src/MobileDetect.php"
	},
	"ResourceModules": {
		"ext.MobileDetect.mobileonly": {
			"styles": "MobileDetect.mobileonly.css"
		},
		"ext.MobileDetect.nomobile": {
			"styles": "MobileDetect.nomobile.css"
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteExtPath": "MobileDetect"
	},
	"Hooks": {
		"BeforePageDisplay": "MobileDetect::addModule",
		"ParserFirstCallInit": "MobileDetect::setParserHook"
	},
	"manifest_version": 2
}
